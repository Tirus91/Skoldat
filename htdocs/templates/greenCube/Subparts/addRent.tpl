<div class="page_name">Pronájmy / nová smluvní strana</div>   
<form method="post" action="#" class="addRoom">
        <div class="section">
            <div class="section_name">Nová smluvní strana</div>
            {tag:status /}
            <div class="formrow"><label for="first_name_rent">Jméno: </label><input name="first_name_rent" value="{tag:first_name_rent /}" /> </div>
            <div class="formrow"><label for="surname_rent">Příjmení: </label><input name="surname_rent" value="{tag:surname_rent /}" /> </div>
            <div class="formrow"><label for="address_rent">Adresa: </label><input name="address_rent" value="{tag:address_rent /}" /> </div>
            <div class="formrow"><label for="town_rent">Město: </label><input name="town_rent" value="{tag:town_rent /}" /> </div>
            <div class="formrow"><label for="phone_rent">Telefon: </label><input name="phone_rent" value="{tag:phone_rent /}" /> </div>
            <div class="formrow"><label for="email_rent">Email: </label><input name="email_rent" value="{tag:email_rent /}" /> </div>
        </div>
    <input name="addRent" class="savebutton" type="submit" value="Zpracovat" />



</form>      
